{
  "version": "1.0",
  "name": "AIHuber",
  "server": {
    "type": "local"
  },
  "endpoints": [
    {
      "identifier": "/proxy/openai",
      "prefix": "/proxy/openai",
      "match": "/proxy/openai/**",
      "backends": [
        {
          "https": [
            {
              "id": "openai-api",
              "url": "https://api.openai.com",
              "ssl": true,
              "methods": ["POST"]
            }
          ]
        }
      ],
      "upstream": {
        "proxy": {
          "enabled": true
        }
      }
    },
    {
      "identifier": "/proxy/mistral",
      "prefix": "/proxy/mistral",
